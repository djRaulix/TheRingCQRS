@using WebSample.Commanding
@model IEnumerable<WebSample.ReadModel.UserIdentity>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.Partial("Create", new CreateUser())
    </p>
    <table>
        <tr>
            <th>First name</th>
            <th>Last name</th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>
                @Html.ActionLink("Details", "Details", new { Id=item.UserIdentityId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
